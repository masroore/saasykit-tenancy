{
    "name": "laravel/laravel",
    "type": "project",
    "description": "SaaSykit is the best starter kit (boilerplate) for your next SaaS project.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4 || ^8.5",
        "aws/aws-sdk-php": "^3.363",
        "blade-ui-kit/blade-icons": "^1.8",
        "filament/filament": "^4.2",
        "filament/spatie-laravel-media-library-plugin": "^4.2",
        "guzzlehttp/guzzle": "^7.10",
        "intervention/image": "^3.11",
        "laragear/two-factor": "^3.0",
        "laravel/framework": "^12.39",
        "laravel/sanctum": "^4.2",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "livewire/flux": "^2.8",
        "livewire/flux-pro": "*",
        "livewire/livewire": "^3.7",
        "mews/purifier": "^3.4",
        "mpociot/versionable": "^4.4",
        "propaganistas/laravel-phone": "^6.0",
        "resend/resend-php": "^1.0",
        "saasykit/filament-breezy": "^2.1",
        "saasykit/filament-country-field": "^1.0",
        "saasykit/laravel-invoices": "^1.0",
        "saasykit/laravel-money": "^1.0",
        "saasykit/laravel-recaptcha": "^1.0",
        "spatie/laravel-cookie-consent": "^3.3",
        "spatie/laravel-flash": "^1.10",
        "spatie/laravel-permission": "^6.23",
        "spatie/laravel-sitemap": "^7.3",
        "stechstudio/filament-impersonate": "^4.0",
        "stripe/stripe-php": "^19.0",
        "symfony/http-client": "^7.4",
        "symfony/mailgun-mailer": "^7.4",
        "symfony/postmark-mailer": "^7.4",
        "twilio/sdk": "^8.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "driftingly/rector-laravel": "^2.1",
        "laravel/pail": "^1.2.3",
        "barryvdh/laravel-debugbar": "^3.16",
        "deployer/deployer": "7.5",
        "doctrine/dbal": "^4.3",
        "fakerphp/faker": "^1.24",
        "kkomelin/laravel-translatable-string-exporter": "^1.25",
        "larastan/larastan": "^3.8",
        "laravel/pint": "^1.25",
        "laravel/sail": "^1.48",
        "laravel/telescope": "^5.15",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "phpunit/phpunit": "^11.0",
        "tightenco/duster": "^3.3" ,
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache": [
            "@php artisan optimize"
        ],
        "reset": [
            "composer clear",
            "composer cache"
        ],
        "fix": "vendor/bin/duster fix",
        "pint": "vendor/bin/pint",
        "rector": "vendor/bin/rector"        
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "flux-pro": {
            "type": "path",
            "url": "./src/flux-pro"
        }
    }
}
